{
  "plugins": ["prettier-plugin-astro", "@trivago/prettier-plugin-sort-imports"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ],
  "importOrder": ["^@?\\w", "^[./]"],
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": true,
  "semi": false
}
